{
   "npm": {
     "publish": false
   },
   "git": {
     "commitMessage": "chore: release v${version}",
     "push": false
   },
   "github":{
     "release": false
   },
   "plugins": {
     "@release-it/conventional-changelog": {
      "header": "# Changelog",
      "preset": {
        "name": "angular"
      },
       "infile": "CHANGELOG.md",
       "ignoreRecommendedBump": true
     }
   }
}
